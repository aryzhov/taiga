page: @html HTML {
	title=Taiga Site
	head [
		GWTScript, MainCss, HistoryFrame, NoGWT
	]

	~GWTScript: SCRIPT {
		type=text/javascript
		lang=javascript
		src=Site/Site.nocache.js
	}

	~MainCss:LINK {
		type=text/css
		rel=stylesheet
		href=taiga.css
	}

	~HistoryFrame:IFRAME {
		src=javascript:''
		id=__gwt_historyFrame
		tabIndex=-1
		style=position:absolute;width:0;height:0;border:0
	}

	~NoGWT:NOSCRIPT [
		=Your web browser must have JavaScript enabled in order for this application to display correctly.
  	]
  	
  	body [
  		P [
  			=Hello!
  			[1,2,3] i -> 
  				A {href=http://google.com} [=Click here
  				i]
  		]
  	]
}